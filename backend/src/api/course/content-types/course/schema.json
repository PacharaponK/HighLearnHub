{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "integer"
    },
    "amount": {
      "type": "integer",
      "default": 0
    },
    "maxamount": {
      "type": "integer"
    },
    "bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking.booking",
      "mappedBy": "course"
    },
    "videos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::video.video",
      "mappedBy": "course"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "subject": {
      "type": "string"
    },
    "detail": {
      "type": "text",
      "required": false
    },
    "study_type": {
      "type": "enumeration",
      "enum": [
        "Online",
        "Live"
      ]
    },
    "instructor_name": {
      "type": "string"
    },
    "watch_times": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::watch-time.watch-time",
      "mappedBy": "course"
    },
    "like": {
      "type": "integer",
      "default": 0
    },
    "schedule_text": {
      "type": "string"
    },
    "promotions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::promotion.promotion",
      "mappedBy": "courses"
    }
  }
}
